<h1>Tasks</h1>
<ul>
  <% @tasks.each do |task| %>
    <li>
      <% if task.completed %>
        <!-- checked box for completed tasks -->
        ✅ <%= task.title %>
      <% else %>
        <!-- empty box for incomplete tasks -->
        ⬜ <%= task.title %>
      <% end %>
    </li>
  <% end %>
</ul>
